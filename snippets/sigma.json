{
	"uuid": {
		"prefix": "uuid",
		"body": [
			"${UUID}"
		],
		"description": "Generate new uuid"
	},
	"references": {
		"prefix": "references:",
		"body": [
			"references:",
			"    - ${CLIPBOARD/(^http.+){0,1}.*/$1/s}"
		],
		"description": "Prints references Skeleton. If a link is in clipboard, it will be used as the first reference"
	}
